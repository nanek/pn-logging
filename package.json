{
  "name": "pn-logging",
  "version": "6.0.0",
  "description": "A logging wrapper around winston and sentry.",
  "engines": {
    "node": ">=10"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/spanishdict/pn-logging.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spanishdict/pn-logging/issues"
  },
  "homepage": "https://github.com/spanishdict/pn-logging",
  "dependencies": {
    "winston": "3.3.3",
    "winston-loggly-bulk": "3.2.1"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "4.2.0",
    "eslint": "7.10.0",
    "istanbul": "0.4.5",
    "mocha": "6.0.2",
    "sinon": "1.17.2",
    "ts-node": "10.4.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "style": "eslint index.ts test/index.ts",
    "test": "env NODE_ENV=test istanbul cover node_modules/.bin/_mocha",
    "check": "npm run style && npm test",
    "watch:test": "env NODE_ENV=test mocha -w --recursive test"
  }
}
